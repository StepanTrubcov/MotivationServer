generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model User {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  telegramId String   @unique
  firstName  String?
  username   String?
  photoUrl   String?
  pts        Int      @default(0)
}